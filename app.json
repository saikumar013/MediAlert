{
  "expo": {
    "name": "MediAlert",
    "slug": "medialert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.medialert.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "e420c810-2427-421a-8caf-b87bdd914b2d"
      }
    },
    "owner": "saiko_013"
  }
}
